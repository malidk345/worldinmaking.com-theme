    "cross-env": "^10.1.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.12",
    "patch-package": "^8.0.1",
    "supabase": "^2.76.8",
    "tailwindcss": "^4",
    "typescript": "5.7.3",
    "wrangler": "^4.65.0"
  }
}
