{
  "name": "posthog-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pnpm run dev:clean",
    "dev:fast": "cross-env NEXT_DISABLE_DEVTOOLS=1 NEXT_DIST_DIR=.next-dev next dev",
    "dev:clean": "powershell -NoProfile -ExecutionPolicy Bypass -Command \"$ports=3000,8001; Get-NetTCPConnection -State Listen -ErrorAction SilentlyContinue | Where-Object { $ports -contains $_.LocalPort } | Select-Object -ExpandProperty OwningProcess -Unique | ForEach-Object { Stop-Process -Id $_ -Force -ErrorAction SilentlyContinue }; if (Test-Path .next-dev) { Remove-Item -Recurse -Force .next-dev }; if (Test-Path .next) { Remove-Item -Recurse -Force .next }; $env:NEXT_DISABLE_DEVTOOLS='1'; $env:NEXT_DIST_DIR='.next-dev'; pnpm exec next dev -p 3000\"",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "postinstall": "patch-package",
    "pages:build": "npx @cloudflare/next-on-pages@1"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@headlessui/tailwindcss": "^0.2.2",
    "@popperjs/core": "^2.11.8",
    "@posthog/icons": "^0.36.5",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-menubar": "^1.1.16",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@supabase/supabase-js": "^2.95.3",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.11",
    "@tailwindcss/typography": "^0.5.19",
    "@tiptap/extension-character-count": "^3.19.0",
    "@tiptap/extension-image": "^3.19.0",
    "@tiptap/extension-link": "^3.19.0",
    "@tiptap/extension-placeholder": "^3.19.0",
    "@tiptap/react": "^3.19.0",
    "@tiptap/starter-kit": "^3.19.0",
    "@types/lodash": "^4.17.23",
    "algoliasearch": "^4.25.3",
    "clsx": "^2.1.1",
    "cntl": "^1.0.0",
    "dayjs": "^1.11.19",
    "framer-motion": "^12.33.0",
    "lodash": "^4.17.23",
    "lucide-react": "^0.563.0",
    "mark.js": "^8.11.1",
    "next": "15.5.12",
    "radix-ui": "^1.4.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-dropzone": "^15.0.0",
    "react-instantsearch-hooks-web": "^6.47.3",
    "react-intersection-observer": "^10.0.2",
    "react-markdown": "^9.1.0",
    "react-popper": "^2.3.0",
    "react-scroll": "^1.9.0",
    "react-scrollspy": "^3.4.3",
    "rehype-raw": "^7.0.0",
    "rehype-sanitize": "^6.0.0",
    "remark-gfm": "^4.0.1",
    "slugify": "^1.6.6",
    "swr": "^2.4.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animated": "^2.0.0"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.16",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "20.19.33",
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.0",
    "@types/react-scroll": "^1.8.10",
